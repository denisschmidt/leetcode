{
  "name": "leetcode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "./node_modules/.bin/eslint \"**/*.js\"",
    "release": "release-it",
    "precommit": "lint-staged",
    "tsc": "tsc --skipLibCheck -w"
  },
  "lint-staged": {
    "*.{ts,js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/node": "^14.0.22",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "eslint": "^5.12.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  }
}
