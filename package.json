{
  "name": "leetcode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "./node_modules/.bin/eslint ./src",
    "release": "release-it",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "eslint": "^5.12.0"
  }
}
