{
  "name": "leetcode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "./node_modules/.bin/eslint \"**/*.js\"",
    "release": "release-it",
    "precommit": "lint-staged",
    "tsc": "tsc --skipLibCheck -w"
  },
  "lint-staged": {
    "*.{ts,js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/node": "^14.0.23",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  }
}
